################################################################################
######################Definition de la fonction factorielle#####################
##############################################################################!#

(defun facto (n)
    (if (< n 1)
 	       1
    	   (* n (fact (- n 1)))))
    	   
################################################################################
#######################Definition de la fonction fibonacci######################
##############################################################################!#
  	   
(defun fibona (n)
    (if (< n 2)
			1
			(+ (fibo (- n 1)) (fibo (- n 2)))))
    	   
